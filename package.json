    "babel-loader": "^7.1.5",
    "babel-preset-env": "*",
    "babel-preset-es2015": "*",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "file-loader": "^3.0.1",
    "mathjs": "^5.4.1",
    "mocha": "*",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
